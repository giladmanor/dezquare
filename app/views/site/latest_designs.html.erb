		<div id="content">
			<div class="page-content">
				<%=render "cover"%>
				<div id="sidebar">
					<a href="/game/index" class="orange-button">Get a new design?</a>
					<div class="title">
						<a href="/site/dashboard">Back to Dashboard</a>
					</div>
				</div>
				<div id="main">
					<div class="white-band-title">
						<h3>Latest Designs</h3>
					</div>
					<div id="match-projects" class="white-box">
					
						<%
						#initiated projects project.status==:started
						
						@user.projects_out.reverse.reject{|p| ["started", "canceled", nil].include?(p.status)  }.each{|project|
							%>
							
							<div class="project">
								<div class="basic">
									<div class="icon">
										<img src="/img/icons/<%=project.category.icon%>.png" />
									</div>
									<div class="name-and-client">
										<div class="name single"><%=project.title%></div>
									</div>
									<div class="action wider">
										<%
										unless ["completed", "delivered"].include?(project.status)
											%><a href="#" class="black-button esmall-font cancel-design" onclick="$('#cancel_id').val('<%=project.id%>')">Cancel Design</a><%
										else
										
										end	
										%>
										
										<a href="" class="pending"><%=project.display_status%></a>
									</div>
								</div>
								<div class="detailed">
									<div class="col1">
										<div class="title">About the desired <%=project.category.name%></div>
										<div class="about-project">
											<%=project.info%>
										</div>
									</div>
									<div class="col2">
										<div class="title">Top Matches</div>
										<div class="top-matches">
											<%
											unless project.game.nil?
												project.game.designers.each{|dez|
													
													%>
													<div class="match <%=(dez.id==project.designer.id) ? "selected" : "disabled"%>">
														<div class="profile-image">
															<img src="/user_avatars/<%= dez.avatar.nil? ? "designer_profile_default.jpg" : dez.avatar%>" alt="My Profile Photo" style="max-width: 56px"/>
														</div>
														<div class="details">
															<div class="big">
																<a target="_blank" href="/designer/profile/<%=dez.id%>"><%=dez.full_name%></a>
																
															</div>
															<div class="small"><%=(dez.location.nil? || dez.location=="") ? '' : "From #{dez.location}"%></div>
															<div class="small">Languages: <%=raw(dez.languages.map{|l| l.name}.join(" <span>&amp;</span> "))%></div>
															
														</div>
														<a style="position:relative; top:5px; left:69px" href="mailto:<%=dez.email%>" class="contact">
																<span id="mailhide" class="icon"></span></a>	
													</div>		
													<%
													}
											else
												%>
												No game 
												<%	
											end
											%>
										</div>
									</div>
								</div>
								<div class="last-row">
									<div class="col1">
										<div class="payment">Cost: <%=project.game.nil? ? 'Unspecified' : "$#{project.game.max_price}"%></div>
									</div>
									<div class="col2">
										<div class="date"><%=project.created_at.strftime("%I:%M%p, %e %b %Y")%></div>
									</div>
								</div>
							</div>		
							<%
						}
						%>		
						
												
						<a href="#" id="dashboard-load-more" class="black-button small-font load-more">
							<span>Show me more</span>
						</a>
					</div>
				</div>
			</div>			
		</div>
		
		<div id="window-overlay"></div>
	
	<div id="cancel-design-popup" class="popup">
		<span class="close"></span>
		<div class="popup-content">
			<div class="title">Are you sure you want<br/>to cancel the design?</div>
			<div class="subtitle">This action canâ€™t be undone</div>
			<div class="buttons">
				<form id="cancel_design_form" action="/site/cancel_design" method="post">
					<input type="hidden" id="cancel_id" name="id"/>
				</form>
				<a href="#" class="back">Back</a>
				<a href="#" class="black-button small-font" onclick="$('#cancel_design_form').submit();return false;">Cancel Design</a>
			</div>
		</div>		
	</div>	
	
	
	