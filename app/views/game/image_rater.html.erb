				<div class="wide-page-holder game-step" id="game-step-3">
					<div class="wide-page-wrap">
						<h2>Let’s get to know your taste!</h2>
						<h4>Select the designs you like</h4>
						<form action="/game/next">
							<div id="image-select" style="text-align: center;width: auto;">
								
								<%
								
								logger.debug "#### #{@game.stage.args}"
								
								pool_size=@game.stage.args[:image_pool_size] || 10
								category = @game.category
								
								images = category.image_grab(pool_size).reject{|i| @game.images.include?(i)}
								logger.debug "--------------------------------------------------------------"
								logger.debug " found #{images.length} images for category #{category.name}"
								
								rand_index = ((Time.now.to_f*1000)%images.length).ceil-1
								
								logger.debug " Random Index #{rand_index}"
								
								
								image=images[rand_index]
								logger.debug " image choosen  #{image.inspect}"
								
								
								logger.debug "--------------------------------------------------------------"
								
								%>
								
								<div style="height: 413px">
									<img src="/repository/<%= image.file_path%>" style="max-width: 672px;max-height: 416px;" />
								</div>
								
								<div class="buttons-select clearfix" style="margin-top: 34px;width: 400px;text-align: center;padding-left: 30%;">
									<a href="/game/next?image_id=<%=image.id%>&selected=pass" class="black-button huge-font" >Pass</a>
									<a href="/game/next?image_id=<%=image.id%>&selected=like" class="orange-button huge-font" >Like</a>
								</div>
							</div>
							<div class="buttons2 clearfix">
								<!-- <a href="game1.html" class="black-button big-font">
									<img src="/img/arrow-left.png" class="normal" />
									<img src="/img/arrow-left-orange.png" class="hover" />
									Back
								</a> -->
								<a href="#" class="black-button big-font disabled next" id="submit">
									Next
									<img src="/img/arrow-right.png" class="normal" />
									<img src="/img/arrow-right-orange.png" class="hover" />
								</a>
							</div>
						</form>
					</div>
				</div>
			